{
  "name": "starter-kit",
  "version": "1.0.2",
  "description": "",
  "main": "index.js",
  "scripts": {
    "prestart": "npm run up",
    "start": "npm-run-all security-check up",
    "poststart": "npm-run-all --parallel webpack:dev-server open:browser",
    "security-check": "nsp check",
    "up": "bower i && yarn",
    "webpack:dev-server": "node_modules/.bin/webpack-dev-server --config webpack.config.js --watch",
    "build": "NODE_ENV=production node_modules/.bin/webpack --progress -p --config webpack.config.prod.js",
    "postbuild": "node ./tools/build-copy-data",
    "open:browser": "node_modules/.bin/babel-node ./tools/open-browser",
    "storybook": "start-storybook -p 8081 -c .storybook"
  },
