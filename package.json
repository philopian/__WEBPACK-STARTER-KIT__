{
  "name": "starter-kit",
  "version": "1.0.2",
  "description": "",
  "main": "index.js",
  "scripts": {
    "prestart": "npm run up",
    "start": "npm-run-all security-check up",
    "poststart": "npm-run-all --parallel webpack:dev-server open:browser",
    "security-check": "nsp check",
    "up": "bower i && yarn",
    "webpack:dev-server": "node_modules/.bin/webpack-dev-server --config webpack.config.js --watch",
    "build": "NODE_ENV=prod node_modules/.bin/webpack --progress -p --config webpack.config.prod.js",
    "postbuild": "node ./tools/build-copy-data",
    "open:browser": "node_modules/.bin/babel-node ./tools/open-browser"
  },
  "author": "Phil Willis <pdotwdot@gmail.com> (http://www.philwillis.pw/)",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-airbnb": "^2.4.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "copy-dir": "^0.3.0",
    "css-loader": "^0.28.7",
    "eslint": "^4.9.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.8.0",
    "extract-text-webpack-plugin": "^3.0.1",
    "file-loader": "^1.1.5",
    "html-loader": "^0.5.1",
    "html-webpack-plugin": "^2.30.1",
    "node-sass": "^4.5.3",
    "npm-run-all": "^4.1.1",
    "nsp": "^2.8.1",
    "open": "^0.0.5",
    "postcss-loader": "^2.0.8",
    "rimraf": "^2.6.2",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.0",
    "url-loader": "^0.6.2",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.9.3"
  },
  "dependencies": {}
}
